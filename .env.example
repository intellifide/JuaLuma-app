# Core local database
POSTGRES_USER=jualuma_user
POSTGRES_PASSWORD=change-me
POSTGRES_DB=jualuma_db
DATABASE_URL=postgresql://jualuma_user:change-me@localhost:5433/jualuma_db

# Backend required app config
APP_ENV=local
FRONTEND_URL=http://localhost:5175
BACKEND_CORS_ORIGINS=http://localhost:5175,http://localhost:5176,http://localhost:5177
GCP_PROJECT_ID=jualuma-dev
GOOGLE_CLOUD_PROJECT=jualuma-dev
GMAIL_IMPERSONATE_USER=hello@jualuma.com
LOCAL_ENCRYPTION_KEY=local-dev-key-change-me
JOB_RUNNER_SECRET=local-job-runner-secret

# Plaid (required by backend settings)
PLAID_CLIENT_ID=plaid-client-id
PLAID_SECRET=plaid-secret
PLAID_ENV=sandbox

# Stripe (set test keys for local)
STRIPE_PUBLISHABLE_KEY=pk_test_replace_me
STRIPE_SECRET_KEY=sk_test_replace_me
STRIPE_WEBHOOK_SECRET=whsec_replace_me

# Tatum hard-cutover provider contract (active Web3 provider)
TATUM_API_KEY=replace_me
